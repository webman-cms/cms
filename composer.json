{
  "name": "webman-cms",
  "type": "project",
  "keywords": [
    "high performance",
    "CMS system"
  ],
  "homepage": "https://github.com/webman-cms",
  "license": "MIT",
  "description": "High performance CMS system.",
  "authors": [
    {
      "name": "weijer",
      "email": "weiwei163@foxmail.com",
      "homepage": "https://github.com/webman-cms",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/webman-cms/cms/issues",
    "source": "https://github.com/webman-cms/cms"
  },
  "require": {
    "php": ">=7.2",
    "workerman/webman-framework": "^1.0",
    "monolog/monolog": "^2.0",
    "vlucas/phpdotenv": "^5.1.0",
    "php-di/php-di": "^6.3",
    "doctrine/annotations": "^1.11",
    "robmorgan/phinx": "^0.12.5",
    "topthink/think-orm": "^2.0",
    "topthink/think-validate": "^1.0.2",
    "illuminate/redis": "^8.2.0",
    "symfony/cache": "^5.2"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
